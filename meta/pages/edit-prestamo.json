                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Prestamo",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          },
                          {
                            "name": "hasChanges",
                            "type": "set",
                            "value": "result is DbUpdateConcurrencyException",
                            "valueType": "bool"
                          },
                          {
                            "name": "canEdit",
                            "type": "set",
                            "condition": "!(result is DbUpdateConcurrencyException)",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "name": "JOSE.updatePrestamo",
                        "parameters": [
                          {
                            "name": "IdPrestamos",
                            "value": "${parameters.IdPrestamos}"
                          },
                          {
                            "name": "Prestamo",
                            "value": "${prestamo}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${prestamo}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "FinalProyect2.Models.Jose.Prestamo",
                  "data": "${prestamo}",
                  "visible": "${prestamo != null && canEdit}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "hasChanges",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "JOSE.getPrestamoByIdPrestamos",
        "parameters": [
          {
            "name": "IdPrestamos",
            "value": "${parameters.IdPrestamos}"
          }
        ],
        "then": [
          {
            "name": "prestamo",
            "type": "set",
            "value": "${result}",
            "valueType": "FinalProyect2.Models.Jose.Prestamo"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "JOSE.getClientes",
        "parameters": [],
        "then": [
          {
            "name": "getClientesForFk_IdClienteResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<FinalProyect2.Models.Jose.Cliente>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Prestamo",
  "layout": "Main"
