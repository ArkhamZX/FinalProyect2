{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "closeLabel",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Item no longer available.",
                  "type": "label",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "type": "close-popup"
                      }
                    ]
                  },
                  "icon": "",
                  "name": "closeButton",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Close",
                  "type": "button",
                  "visible": "${!canEdit}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "components": [
            {
              "class": "col-md-9",
              "components": [
                {
                  "name": "label0",
                  "style": {
                    "fontWeight": "bold"
                  },
                  "text": "Another user has made conflicting changes to one or more of the fields you have modified. Please reload.",
                  "type": "label",
                  "visible": "${hasChanges}"
                }
              ],
              "name": "col0",
              "type": "col"
            },
            {
              "class": "col-md-3",
              "components": [
                {
                  "buttonStyle": "secondary",
                  "events": {
                    "click": [
                      {
                        "code": "Jose.Reset()",
                        "type": "execute"
                      },
                      {
                        "code": "await this.Load()",
                        "type": "execute"
                      }
                    ]
                  },
                  "icon": "autorenew",
                  "name": "button0",
                  "style": {
                    "marginBottom": "20px"
                  },
                  "text": "Reload",
                  "type": "button",
                  "visible": "${hasChanges}"
                }
              ],
              "name": "col1",
              "type": "col"
            }
          ],
          "name": "row0",
          "style": {},
          "type": "row"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Id Garantia",
                              "component": "IdGarantia",
                              "name": "IdGarantiaLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "IdGarantia",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${garantium.IdGarantia}"
                            },
                            {
                              "type": "required-validator",
                              "name": "IdGarantiaRequiredValidator",
                              "component": "IdGarantia",
                              "text": "IdGarantia is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Tipo",
                              "component": "Tipo",
                              "name": "TipoLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 250,
                              "type": "textbox",
                              "name": "Tipo",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${garantium.Tipo}"
                            },
                            {
                              "type": "required-validator",
                              "name": "TipoRequiredValidator",
                              "component": "Tipo",
                              "text": "Tipo is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Valor",
                              "component": "Valor",
                              "name": "ValorLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 250,
                              "type": "textbox",
                              "name": "Valor",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${garantium.Valor}"
                            },
                            {
                              "type": "required-validator",
                              "name": "ValorRequiredValidator",
                              "component": "Valor",
                              "text": "Valor is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Ubicacion",
                              "component": "Ubicacion",
                              "name": "UbicacionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 250,
                              "type": "textbox",
                              "name": "Ubicacion",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${garantium.Ubicacion}"
                            },
                            {
                              "type": "required-validator",
                              "name": "UbicacionRequiredValidator",
                              "component": "Ubicacion",
                              "text": "Ubicacion is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Prestamo",
                              "component": "Fk_IdPrestamos",
                              "name": "Fk_IdPrestamosLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "data": "${getPrestamosForFk_IdPrestamosResult}",
                              "textProperty": "Periodo",
                              "valueProperty": "IdPrestamos",
                              "placeholder": "Choose Prestamo",
                              "valueType": "int",
                              "showSearch": false,
                              "type": "dropdown-datagrid",
                              "name": "Fk_IdPrestamos",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${garantium.Fk_IdPrestamos}"
                            },
                            {
                              "type": "required-validator",
                              "name": "Fk_IdPrestamosRequiredValidator",
                              "component": "Fk_IdPrestamos",
                              "text": "Fk_IdPrestamos is required",
                              "style": {
                                "position": "absolute"
                              },
                              "defaultValue": "0"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Fecha Creacion",
                              "component": "FechaCreacion",
                              "name": "FechaCreacionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime?",
                              "dateFormat": "dd/MM/yyyy",
                              "type": "datepicker",
                              "name": "FechaCreacion",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${garantium.FechaCreacion}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Garantium",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          },
                          {
                            "name": "hasChanges",
                            "type": "set",
                            "value": "result is DbUpdateConcurrencyException",
                            "valueType": "bool"
                          },
                          {
                            "name": "canEdit",
                            "type": "set",
                            "condition": "!(result is DbUpdateConcurrencyException)",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "name": "JOSE.updateGarantium",
                        "parameters": [
                          {
                            "name": "IdGarantia",
                            "value": "${parameters.IdGarantia}"
                          },
                          {
                            "name": "Garantium",
                            "value": "${garantium}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${garantium}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "FinalProyect2.Models.Jose.Garantium",
                  "data": "${garantium}",
                  "visible": "${garantium != null && canEdit}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "hasChanges",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "JOSE.getGarantiumByIdGarantia",
        "parameters": [
          {
            "name": "IdGarantia",
            "value": "${parameters.IdGarantia}"
          }
        ],
        "then": [
          {
            "name": "garantium",
            "type": "set",
            "value": "${result}",
            "valueType": "FinalProyect2.Models.Jose.Garantium"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "JOSE.getPrestamos",
        "parameters": [],
        "then": [
          {
            "name": "getPrestamosForFk_IdPrestamosResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<FinalProyect2.Models.Jose.Prestamo>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Garantium",
  "layout": "Main"
}